<odoo>
  <data noupdate="1">
    <record id="ir_cron_sync_bed_charges" model="ir.cron">
      <field name="name">Sync Bed Charges from OpenMRS</field>
      <field name="model_id" ref="model_bed_sync_engine"/>
      <field name="state">code</field>
      <field name="code">model.sync_bed_charges()</field>
      <field name="interval_number">5</field>
      <field name="interval_type">minutes</field>
      <field name="numbercall">-1</field>
      <field name="active" eval="True"/>
    </record>
  </data>
</odoo>
